{
  "name": "angular-17-starter",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "extract-translations": "ngx-translate-extract --input ./src --output src/assets/i18n/en-CA.json --clean --sort --format --key-as-default-value namespaced-json --marker _ && ngx-translate-extract --input ./src --output src/assets/i18n/fr-CA.json --clean --sort --format --key-as-default-value namespaced-json --marker _",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
